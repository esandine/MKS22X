import java.util.*;
@SuppressWarnings("unchecked")
public class MyHeap<T extends Comparable<T>>{
    private T[] data;
    private int size;
    private boolean max;
    private boolean compare(T t1, T t2){
	if(max){
	    return t1.compareTo(t2)>=0;
	}else{
	    return t1.compareTo(t2)<=0;
	}
    }
    public MyHeap(){
	data = new T[10];
	
    }

}
