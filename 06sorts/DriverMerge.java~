public class DriverMerge{
    public static void main(String[]args){
	int [] data = {6,5,4,3};
	Sorts.printArray(data);
	int [] data2 = data;
	Sorts.printArray(data2);
	data2=Sorts.merge(data,0,0,2,2);
	Sorts.printArray(data2);
	data2=Sorts.merge(data,2,2,3,3);
	Sorts.printArray(data2);
	data=Sorts.merge(data2,0,1,2,3);
	Sorts.printArray(data);
    }
}
